{
  "cache": false,
  "check-coverage": false,
  "report-dir": "./reports/coverage",
  "extension": [
    ".ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "reports/**",
    "node_modules/**",
    "**/*.d.ts",
    "**/*.test.ts"
  ],
  "sourceMap": true,
  "reporter": [
    "lcov",
    "text",
    "text-summary"
  ],
  "all": true,
  "instrument": true
}